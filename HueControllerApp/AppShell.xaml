<Shell
    x:Class="HueControllerApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HueController.Views"
    Title="HueControllerApp">

    <ShellContent
        Title="Connect"
        ContentTemplate="{DataTemplate local:ConnectPage}"
        Route="ConnectPage" />

    <ShellContent
        Title="Main"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
</Shell>
